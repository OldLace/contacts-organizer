<h1>Your Business Contacts</h1>
<p>Find me in app/views/biz_contacts/index.html.erb</p>

<% @contacts.each do |contact| %>

  <h5><%= contact.first_name %></h5>
  <p><%= contact.last_name %></p>
  <p><%= contact.phone %></p>
  <p><%= contact.email %></p>
  <p><%= contact.address %></p>
  <hr>
<% end  %>
